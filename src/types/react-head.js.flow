declare module "react-head" {
	import type { Node, ComponentType, Element } from "react"

	declare export var HeadProvider : ComponentType<{
		headTags? : Array<Element<any>>,
		children : React.Node,
	}>

	declare export var Title : React.ComponentType<{ children : Node }>
	declare export var Style : React.ComponentType<{ children : Node }>

	declare export var Link : ComponentType<{
		rel : string,
		href : string,
		...
	}>

	declare export var Meta : ComponentType<{
		name : string,
		content : string,
		...
	}>
}
